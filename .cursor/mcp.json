{
  "mcpServers": {
    "igniter-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@igniter-js/mcp-server@latest"
      ],
      "env": {
        "GOOGLE_API_KEY": "",
        "GITHUB_TOKEN": ""
      }
    },
    "browser": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp@latest"],
      "env": {}
    },
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    },
    "@magicuidesign/mcp": {
      "command": "npx",
      "args": ["-y", "@magicuidesign/mcp@latest"]
    }
  }
}
